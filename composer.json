{
  "name": "voku/kint",
  "description": "Kint - debugging helper for PHP developers",
  "keywords": [
    "kint",
    "php",
    "debug"
  ],
  "type": "library",
  "homepage": "https://github.com/raveren/kint",
  "license": "MIT",
  "authors": [
    {
      "name": "Rokas Å leinius",
      "homepage": "https://github.com/raveren"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/raveren/kint/contributors"
    }
  ],
  "repositories": {
    "portable-utf8": {
      "type": "vcs",
      "url": "https://github.com/menadwork/portable-utf8"
    }
  },
  "require": {
    "php": ">=5.3.0",
    "portable-utf8": "~3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "psr-4": {
      "kint\\": "src/kint/"
    },
    "files": [
      "bootstrap.php"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  }
}
